<h1>New user!</h1>

<form action="<%= users_url %>" method="post">

  <label for="email">Email</label>
  <input
    type="text"
    name="user[email]"
    id="email"
    value="">

  <label for="password">Password</label>
  <input
    type="password"
    name="user[password]"
    id="password"
    value="">
  <br>

  <input type="submit" value="Sign Up!">
</form>